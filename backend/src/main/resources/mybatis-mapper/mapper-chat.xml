  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lx.finalproject.dao.ExampleDao">

   <!-- 채팅 -->
	<insert id="createChat"
		parameterType="com.lx.finalproject.vo.ChatMessageVo">
		INSERT INTO chat user_name, message, room_id
		time)
		VALUES (#{userName}, #{message}, #{roomId}, #{time});
	</insert>
  
